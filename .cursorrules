{
  "name": "meal-planner",
  "type": "web",
  "framework": "react",
  "styling": "tailwindcss",
  "deployment": "vercel",
  "dependencies": {
    "frontend": [
      "react",
      "react-dom",
      "react-beautiful-dnd",
      "react-pdf",
      "@material-ui/core",
      "tailwindcss",
      "firebase"
    ],
    "development": [
      "typescript",
      "eslint",
      "prettier"
    ]
  },
  "structure": {
    "src": {
      "components": [
        "calendar",
        "meals",
        "recipes",
        "auth",
        "pdf",
        "common"
      ],
      "pages": [
        "Home",
        "WeeklyPlanner",
        "MealLibrary",
        "Profile"
      ],
      "services": [
        "firebase",
        "pdf",
        "meals"
      ],
      "utils": [
        "helpers",
        "constants"
      ],
      "types": [
        "meals",
        "user",
        "plan"
      ]
    }
  },
  "rules": {
    "lint": {
      "semi": true,
      "trailingComma": "es5",
      "singleQuote": true,
      "printWidth": 100,
      "tabWidth": 2
    },
    "typescript": {
      "strict": true
    }
  },
  "testing": {
    "framework": "jest",
    "coverage": 80
  }
} 